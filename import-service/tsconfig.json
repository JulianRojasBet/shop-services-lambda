{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": [ "ES6" ],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "baseUrl": "./",
    "paths": {
      "functions/*": ["./functions/*"],
      "constants/*": ["./constants/*"],
    }
  }
}
